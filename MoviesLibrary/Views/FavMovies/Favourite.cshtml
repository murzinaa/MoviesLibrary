@using APIProviders.MovResultById;
@model List<MovieResultById>

@for (int i = 0; i < Model.Count; i ++)
{
    <form method="post">
        <input type="hidden" name="movie" value=@Model[i].id />
        <button type="submit" class="alert-dark" name="movie" asp-controller="Categories" asp-action="MovieResult">@Model[i].title</button>

    </form>
}